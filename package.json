{
	"name": "nimbus-api",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
	"scripts": {
		"serve": "node build/app.js",
		"serve:dev": "doppler run -- node build/app.js",
		"start": "npm run build:live",
		"dev": "doppler run -- nodemon source/app.ts",
		"build": "npm run lint && npm run build-ts && npm run build-cp",
		"build-ts": "tsc -p .",
		"build-cp": "cp -R ./source/public build/",
		"lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
		"build:live": "nodemon source/app.ts",
		"clean": "tsc --build --clean",
		"watch-ts": "tsc -w",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"auth0": "^3.3.0",
		"axios": "^0.27.2",
		"cassandra-driver": "^4.6.4",
		"dotenv": "^16.0.3",
		"express": "^4.18.1",
		"express-oauth2-jwt-bearer": "^1.3.0",
		"method-node": "^0.3.15",
		"nodemon": "^2.0.16",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.4"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/express": "^4.17.13",
		"@types/node": "^18.14.0",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"prettier": "^2.8.4"
	}
}
